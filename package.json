{
  "name": "discord-queue-bot",
  "version": "2.3.0",
  "description": "A Discord bot for queue management.",
  "main": "dist/main.ts",
  "scripts": {
    "build": "tsc -p .",
    "clean": "rm -rv dist",
    "dev": "nodemon src/main.ts",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "lint:fix": "eslint . --ext .ts --fix",
    "prune": "rm -rv dist node_modules",
    "prod": "node dist/main.js",
    "start": "ts-node src/main.ts",
    "watch": "tsc --watch",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RaniAgus/discord-queue-bot.git"
  },
  "author": "Agustin Ranieri",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RaniAgus/discord-queue-bot/issues"
  },
  "homepage": "https://github.com/RaniAgus/discord-queue-bot#readme",
  "dependencies": {
    "@discordjs/builders": "^0.15.0",
    "@discordjs/collection": "^0.7.0",
    "@discordjs/rest": "^0.3.0",
    "axios": "^0.27.2",
    "dayjs": "^1.11.3",
    "discord-api-types": "^0.34.0",
    "discord.js": "^13.8.0",
    "dotenv": "^16.0.1",
    "rxjs": "^7.5.5",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^7.0.4",
    "nodemon": "^2.0.19",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  }
}
