{
  "name": "discord-queue-bot",
  "version": "2.3.0",
  "description": "A Discord bot for queue management.",
  "main": "dist/main.ts",
  "scripts": {
    "build": "tsc -p .",
    "build:debug": "tsc -p . --sourcemap",
    "clean": "rm -rv dist",
    "dev": "nodemon src/main.ts",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "lint:fix": "eslint . --ext .ts --fix",
    "prune": "rm -rv dist node_modules",
    "prod": "node dist/main.js",
    "start": "ts-node src/main.ts",
    "watch": "tsc --watch",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RaniAgus/discord-queue-bot.git"
  },
  "author": "Agustin Ranieri",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RaniAgus/discord-queue-bot/issues"
  },
  "homepage": "https://github.com/RaniAgus/discord-queue-bot#readme",
  "dependencies": {
    "@discordjs/collection": "^1.0.1",
    "axios": "^0.27.2",
    "dayjs": "^1.11.3",
    "discord.js": "^14.2.0",
    "dotenv": "^16.0.1",
    "rxjs": "^7.5.5",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.19.3/xlsx-0.19.3.tgz"
  },
  "devDependencies": {
    "@types/node": "^16.11.47",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^7.0.4",
    "nodemon": "^2.0.12",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}
